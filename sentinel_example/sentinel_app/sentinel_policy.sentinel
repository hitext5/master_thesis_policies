plug_slots = 3
plug_rated_power = 20
electronic_device_work_power = 30
input_act = "plug_in"

enough_power = rule {
    plug_rated_power >= electronic_device_work_power
}

enough_slots = rule {
    plug_slots >= 1
}

correct_action = rule {
    input_act == "plug_in"
}

main = rule {
    enough_power and enough_slots and correct_action
}